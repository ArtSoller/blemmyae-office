    "extra": {
        "enable-patching": true,
        "patches-file": "composer.patches.json",
        "installer-paths": {
            "wp-content/mu-plugins/{$name}": [
                "type:wordpress-muplugin"
            ],
            "wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ],
            "wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ]
        },
        "composer-exit-on-patch-failure": true
    },
    "config": {
        "process-timeout": 0,
        "allow-plugins": {
            "pivvenit/acf-pro-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "johnpbloch/wordpress-core-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "scripts": {
        "mono": [
            "vendor/bin/monorepo-builder"
        ],
        "mono-bi": [
            "@mono bump-interdependency"
        ],
        "mono-m": [
            "@mono merge"
        ],
        "mono-r": [
            "@mono release"
        ],
        "mono-rp": [
            "@mono release patch"
        ],
        "mono-rt": [
            "@mono --dry-run release"
        ],
        "mono-pa": [
            "@mono package-alias"
        ],
        "mono-pj": [
            "@mono packages-json"
        ],
        "mono-v": [
            "@mono validate"
        ],
        "packages-symlink": [
            "ls -d packages/* | cut -f2 -d '/' | xargs -I % sh -c '(rm -f wp-content/plugins/% || echo \"[%] symlink was already purged or does not exist\") && ln -s $(pwd)/packages/% wp-content/plugins/%'"
        ],
        "patch": [
            "vendor/bin/vendor-patches generate"
        ],
        "wordpress-post-install": [
            "cat scripts/wp-core-cleanup-list.txt | xargs -I % sh -c '[ -d % ] && rm -r % || echo % was already cleaned up'",
            "[ -d wordpress ] && (rm -r wordpress/wp-content && rm wordpress/composer.json && mv wordpress/* . && rm -r wordpress) || echo Forgot to run composer update?",
            "[ -d wp-content/mu-plugins/composer-autoload ] && (mv wp-content/mu-plugins/composer-autoload/composer-autoload.php wp-content/mu-plugins/composer-autoload.php && rm -r wp-content/mu-plugins/composer-autoload) || echo Forgot to run composer update?",
            "[ -d wp-content/plugins/advanced-ads/modules/gutenberg ] && (rm -r wp-content/plugins/advanced-ads/modules/gutenberg) || echo Forgot to run composer update?"
        ],
        "post-install-cmd": [
            "@wordpress-post-install"
        ],
        "post-update-cmd": [
            "@wordpress-post-install"
        ],
        "test-blemmyae-blocks": [
            "./vendor/bin/phpunit ./packages/blemmyae-blocks/tests/Unit"
        ],
        "phpcs": [
            "phpcs --file-list=phpcs/scopes/blem.txt --standard=phpcs/phpcs.xml -v --extensions=php"
        ],
        "phpcbf": [
            "phpcbf --file-list=phpcs/scopes/blem.txt --standard=phpcs/phpcs.xml -v --extensions=php"
        ],
        "codegen": [
            "/bin/bash scripts/codegen/codegen.sh"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse -c phpstan.neon --memory-limit=1G"
        ]
    },
