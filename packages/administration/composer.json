{
    "name": "cra/administration",
    "description": "Re-usable core functionality for admin and non-admin pages",
    "minimum-stability": "stable",
    "license": "proprietary",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "alexandrkucherov",
            "email": "avdkucherov@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Scm\\Acf_Extended\\": "includes/acf-extended/",
            "Scm\\Advanced_Ads\\": "includes/advanced-ads/",
            "Scm\\Advanced_Custom_Fields\\": "includes/advanced-custom-fields/",
            "Scm\\Archived_Post_Status\\": "includes/archived-post-status/",
